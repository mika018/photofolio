<body class="login_body">
	<div class="login_site login_init">
            <div class="page_0 limiter login_container">
                    <div class="login_wrapper p-t-90 p-b-30">
                        <div class="login_form login_validate-form">


                            <div  id = "email_container" class="login_wrap_input m-b-16" >
                                <input class="login_input" type="text" name="username" placeholder="username" id= "login_email">
                               
                            </div>
                        
                            <div id = "password_container" class="login_wrap_input m-b-20" data-validate = "Please enter password">
                                <input class="login_input" type="password" name="pass" placeholder="password" id= "login_pass">
                            
                            </div>

                            <div id = "login_button_container" class="login_container_btn">
                                <button class="login_btn" id = "login_btn">
                                    login
                                </button>
                            </div>
                            <span class="login_form-title-2">
                                or
                            </span>

                            <div class="album_id_wrap_input  m-b-20" data-validate = "Please enter password">
                                <input class="album_id_input" type="password" name="photographer" placeholder="photographer" id= "user_id"> 
                                <input class="album_id_input" type="password" name="album name" placeholder="album name" id= "album_id">
                            </div>
                            <div id = "album_id_container_btn" class="album_id_container_btn">
                                <button class="album_id_btn" id = "album_id_btn">
                                    find album
                                </button>
                            </div>
                        </div>
                    </div>
            </div>
    </div>
    <a id="load_home" href="/"></a>
    <a id="load_album" ></a>
